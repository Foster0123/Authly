{
	"overrides": [
	  {
		"files": ["src/client/assets/scss/**/*.scss"],
		"customSyntax": "postcss-scss",
		"plugins": [
		  "stylelint-order"
		],
		"extends": ["stylelint-config-standard"],
		"rules": {
		  "order/order": [
			"custom-properties",
			"declarations"
		  ],
		  "order/properties-alphabetical-order": true,
		  "at-rule-no-unknown":null
		}
	  }
	]
  }
  